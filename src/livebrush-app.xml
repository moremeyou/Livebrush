<?xml version="1.0" encoding="UTF-8" standalone="no" ?><application xmlns="http://ns.adobe.com/air/application/2.0">

  <id>com.livebrush</id>

  <version>1.5</version>

  <filename>Livebrush</filename>

  <description/>

  <name>Livebrush Mini</name>

  <copyright>David Fasullo</copyright>

  <initialWindow>
    <content>livebrush.swf</content>
    <systemChrome>standard</systemChrome>
    <transparent>false</transparent>
    <visible>true</visible>
    <maximizable>true</maximizable>
    <minimizable>true</minimizable>
    <resizable>true</resizable>
  </initialWindow>

  <icon>
    <image128x128>AppIcons/appIcon-128.png</image128x128>
    <image48x48>AppIcons/appIcon-48.png</image48x48>
    <image32x32>AppIcons/appIcon-32.png</image32x32>
    <image16x16>AppIcons/appIcon-16.png</image16x16>
  </icon>

  <customUpdateUI>false</customUpdateUI>

  <allowBrowserInvocation>false</allowBrowserInvocation>

  <fileTypes>
    <fileType>
      <name>Livebrush.XML</name>
      <extension>lbp</extension>
      <description>Livebrush Project File</description>
      <contentType>text/xml</contentType>
      <icon>
        <image128x128>FileTypeIconsForAIRPublish/File-Icon-128.png</image128x128>
        <image16x16>FileTypeIconsForAIRPublish/File-Icon-16.png</image16x16>
        <image32x32>FileTypeIconsForAIRPublish/File-Icon-32.png</image32x32>
        <image48x48>FileTypeIconsForAIRPublish/File-Icon-48.png</image48x48>
      </icon>
    </fileType>
  </fileTypes>



</application>
